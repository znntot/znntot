public class BitQuark {
		private int bit; // 0 or 1
		private String color; // Red, Green, Blue
		private String charge; // +, -, 0
		private String spin; // up, down

		public BitQuark(int bit, String color, String charge, String spin) {
			this.bit = bit;
			this.color = color;
			this.charge = charge;
			this.spin = spin;
		}
	}